    <meta name="renderer" content="webkit">
    <title>查看相册照片</title>
    <link rel="stylesheet" href="../css/pintuer.css">
    <link rel="stylesheet" href="../css/admin.css">
    <script src="../js/jquery.js"></script>
    <script src="../js/pintuer.js"></script>
    <script type="text/javascript">
        $(function () {
            //背景低亮
//            $(".custom-mask").css("height", $(document).height());
//            $(window).scroll(function () {
//                $(".custom-mask").css("height", $(document).height());
//            });
            //点击图片查看大图
            $("img").click(function () {
                var imgSrc = $(this).attr("src");
                $(".custom-dialog img").attr("src", imgSrc).load(function () {
                    var ml = ($(window).width() - $(this).width()) / 2;
                    var tp = ($(window).height() - $(this).height()) / 2;
                    if(tp >= 0){
                        //在恰当的位置显示
                        $(".custom-dialog").css({"margin-left": ml, "top": tp});
                    }else{
                        $(".custom-dialog").css({"margin-left": ml, "top": "20px", "padding-bottom":"30px"});
                    }
                }).click(function () {
                    $(".custom-mask, .custom-dialog, .close").hide();
                });
                $(".custom-mask, .custom-dialog, .close").show();
            });
            $(".close").click(function () {
                $(".custom-mask, .custom-dialog, .close").hide();
            });
        });
    </script>
    <style>
        /*CSS3实现瀑布流*/
        .container {
            width: 100%;
            background: #eee;
        }

        /*瀑布流层*/
        .waterfall {
            -moz-column-count: 4; /* Firefox */
            -webkit-column-count: 4; /* Safari 和 Chrome */
            column-count: 4;
            -moz-column-gap: 1em;
            -webkit-column-gap: 1em;
            column-gap: 1em;
        }

        /*一个内容层*/
        .item {
            background: #ffffff;
            padding: 1em;
            margin: 0 0 1em 0;
            -moz-page-break-inside: avoid;
            -webkit-column-break-inside: avoid;
            break-inside: avoid;
        }

        .item img {
            cursor: pointer;
            width: 100%;
            margin-bottom: 10px;
        }

        /*自定义照片大图查看模态框*/
        .custom-mask {
            display: none;
            top: 0;
            position: fixed;
            width: 100%;
            height: 100%;
            background: #000000;
            opacity: 0.7;
        }
        .custom-dialog {
            background-color: rgba(0, 0, 0, 0) !important;
            display: none;
            position: fixed;
            border-radius: 4px;
            background-color: #fff;
            max-width: 80%;
        }
        .close {
            position: absolute;
            color: #fff;
            top:10px;
            right: 10px;
            display: none;
            font-size: 50px;
        }
        .go-back{
            float: right;
            margin-right: 20px;
        }
    </style>
</head>
