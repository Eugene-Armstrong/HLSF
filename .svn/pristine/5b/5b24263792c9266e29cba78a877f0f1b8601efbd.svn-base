package cn.edu.bnuz.seawas.vo.wechat;

import cn.edu.bnuz.seawas.entity.Album;
import cn.edu.bnuz.seawas.entity.Photo;

/**
 * 微信端相片列表Vo
 * Created by sjk on 2017-09-06.
 */
public class AlbumVo {

    private Album album;    //相册信息

    private Integer photoCount;     //相册中的图片数量

    private Photo latestPhoto;   //相册中最新的一样图片

    public Album getAlbum() {
        return album;
    }

    public void setAlbum(Album album) {
        this.album = album;
    }

    public Integer getPhotoCount() {
        return photoCount;
    }

    public void setPhotoCount(Integer photoCount) {
        this.photoCount = photoCount;
    }

    public Photo getLatestPhoto() {
        return latestPhoto;
    }

    public void setLatestPhoto(Photo latestPhoto) {
        this.latestPhoto = latestPhoto;
    }

    @Override
    public String toString() {
        return "AlbumVo{" +
                "album=" + album +
                ", photoCount=" + photoCount +
                '}';
    }
}
