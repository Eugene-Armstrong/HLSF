    <if test="title != null" >
      title = #{title,jdbcType=VARCHAR},
    </if>
    <if test="picUrl != null" >
      picUrl = #{picUrl,jdbcType=VARCHAR},
    </if>
    <if test="state != null" >
      state = #{state,jdbcType=VARCHAR} AND
    </if>
    <if test="remark != null" >
      remark = #{remark,jdbcType=VARCHAR} AND
    </if>
    t_travelnote.userId = b.id
    <if test="adminId != null" >
    AND t_travelnote.adminId = c.id
    </if>
    AND t_travelnote.delFlag = '0'
  </select>

  <!--批量删除-->
