package cn.edu.bnuz.seawas.dao;

import cn.edu.bnuz.seawas.entity.City;

import java.util.List;

/**
 * 城市表DAO接口
 * Created by sjk on 2017-07-28.
 */
public interface CityMapper {

    /**
     * 删除
     * @param city city
     */
    int delete(City city);

    /**
     * 插入数据
     * @param city city
     */
    int insert(City city);

    /**
     * 非空插入
     * @param city city
     */
    int insertSelective(City city);

    /**
     * 获取单个信息
     * @param city city
     */
    City get(City city);

    /**
     * 非空更新
     * @param city city
     */
    int updateByPrimaryKeySelective(City city);

    /**
     * 更新数据
     * @param city city
     */
    int update(City city);

    /**
     * 列表查询
     * @param city city
     */
    List<City> findList(City city);
}