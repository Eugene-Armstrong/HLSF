<script src="js/fastclick.js"></script>
<script>
    $(function () {
        FastClick.attach(document.body);
        // 相册宽高
        var $tab3 = $("#tab3"),
            $weuiTab = $(".weui-tab__bd-item"),
            _h = ($weuiTab.width()-10)/2-20;
        $tab3.find("img").each(function () {
            $(this).css("height",_h);
        });
        $(window).resize(function () {
            _h = ($weuiTab.width()-10)/2-20;
            $tab3.find("img").each(function () {
                $(this).css("height",_h);
            })
        });
    })
</script>
