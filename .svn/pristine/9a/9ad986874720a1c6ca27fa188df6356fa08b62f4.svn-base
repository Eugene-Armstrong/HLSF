    </select>

    <select id="findList" resultType="cn.edu.bnuz.seawas.entity.Product">
        SELECT
        <include refid="Base_Column_List"/>
        FROM t_product a
        <where>
            <if test="delFlag != null and delFlag != ''">
                a.delFlag = #{delFlag}
            </if>
            <if test="name != null and name != ''">
                AND a.name LIKE concat(concat('%',#{name}),'%')
            </if>
        </where>
    </select>

